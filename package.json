    "@types/pg": "^8.15.6",
    "kysely-codegen": "^0.19.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.7.4",
    "chardet": "^2.1.1",
    "date-fns": "^4.1.0",
    "hono": "^4.10.3",
    "iconv-lite": "^0.7.0",
    "kysely": "^0.28.8",
    "lucide-react": "^0.548.0",
    "papaparse": "^5.5.3",
    "pg": "^8.16.3",
    "resend": "^6.3.0",
    "ulid": "^3.0.1",
    "zod": "^4.1.12"
  }
